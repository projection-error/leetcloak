{
  "name": "LeetCloak",
  "description": "Override Leetcode difficulty rating.",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "favicon.png"
  },

  "permissions": [
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "https://leetcode.com/problems/*",
    "http://leetcode.com/problems/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://leetcode.com/problems/*",
        "http://leetcode.com/problems/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "styles.css"
      ],
       
      "run_at": "document_end"
    }
  ]
}